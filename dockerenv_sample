# Environment variables for Docker Compose (update!)

# Naked domain is fine here. Nginx will also target www.~
DOMAIN_NAME=example.com

# Database Credentials
POSTGRES_USER=sampleuser
POSTGRES_PASSWORD=samplepassword
POSTGRES_DB=sampledb
POSTGRES_HOST=localhost

# App service container port.
PORT=5000

# 0 (DEBUG mode OFF) or 1 (ON). Set this to 0 in production!
DEBUG=0
